  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#3FA34D"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#2C662D"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#519259"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.tag",
        "support.function"
      ],
      "settings": {
        "foreground": "#27632A"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#4C8B3A"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#529658"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#67AA6C"
      }
    }
  ]
