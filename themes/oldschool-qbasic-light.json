  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6A737D"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    }
  ]
