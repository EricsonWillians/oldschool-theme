  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.tag",
        "support.function"
      ],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFFF00"
      }
    }
  ]
}