{
    "name": "Windows 95 (Somewhat nostalgic)",
    "type": "dark",
    "colors": {
      "editor.background": "#000000",
      "editor.foreground": "#FFFFFF",
      "editorLineNumber.foreground": "#808080",
      "editorCursor.foreground": "#FFFFFF",
      "editor.lineHighlightBackground": "#555555",
      "selection.background": "#FF8C00",
      "activityBar.background": "#000080",
      "activityBar.foreground": "#FFFFFF",
      "activityBarBadge.background": "#FFA500",
      "activityBarBadge.foreground": "#000000",
      "tab.activeForeground": "#FFFFFF",
      "tab.inactiveForeground": "#808080",
      "tab.border": "#FFFFFF",
      "statusBar.background": "#000080",
      "statusBar.foreground": "#FFFFFF",
      "sideBar.background": "#000080",
      "sideBar.foreground": "#FFFFFF",
      "sideBar.border": "#FFFFFF",
      "terminal.foreground": "#FFFFFF"
    },
    "tokenColors": [
      {
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "foreground": "#808080"
        }
      },
      {
        "scope": "string",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "constant.numeric",
        "settings": {
          "foreground": "#00FF00"
        }
      },
      {
        "scope": [
          "keyword",
          "storage.type"
        ],
        "settings": {
          "foreground": "#00FFFF"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "entity.name.tag",
          "support.function"
        ],
        "settings": {
          "foreground": "#FF00FF"
        }
      },
      {
        "scope": "variable",
        "settings": {
          "foreground": "#FFA500"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#808080"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#FFFF00"
        }
      }
    ]
  }
  